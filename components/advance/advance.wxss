.childs {
    position: relative;
    display: flex;
    line-height: 35px;
}

.changeImg {
    width: 14px;
    height: 14px;
}

.changeImg>image {
    width: 100%;
    height: 100%;
}

.ul {
    padding-left: 0rpx;
    position: relative;
}

.li {
    list-style-type: none;
    margin: 0;
    padding: 15rpx 50rpx 0;
    position: relative;
    line-height: 40rpx;
}

.li::before,.li::after {
    content: '';
    left: -16rpx;
    position: absolute;
    right: auto;
}

.li::before {
    border-left: 1rpx dashed #999;
    bottom: 100rpx;
    height: 100%;
    top: -17rpx;
    width: 2rpx;
}

.li::after {
    border-top: 1rpx dashed #999;
    height: 40rpx;
    top: 50rpx;
    width: 40rpx;
}

.li .span {
    width: 603rpx;
    text-align: left;
    height: 66rpx;
    line-height: 66rpx;
    display: inline-block;
    padding: 6rpx 16rpx;
    text-decoration: none;
    font-size: 32rpx;
    position: relative;
}

.ul>.li1::before,.ul>.li1::after {
    border-top: 1rpx dashed #999;
    height: 40rpx;
    top: 50rpx;
    width: 0rpx;
    border-left: 0;
}

.li:last-child::before {
    height: 68rpx;
}

.texts {
    width: 45rpx;
    height: 45rpx;
    line-height: 45rpx;
    position: absolute;
    right: -8%;
    top: -28%;
    border-radius: 50%;
    background-color: #afafaf;
    font-size: 27rpx;
    color: #fff;
}

.dai {
    position: absolute;
    font-size: 27rpx;
    left: 18rpx;
    top: 36rpx;
    z-index: 999;
    padding: 0 8rpx;
    display: flex;
    align-items: center;
}

.plus {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
}

.name {
    width: 80%;
    box-sizing: border-box;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}