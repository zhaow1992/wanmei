.container {
    min-height: 0;
}

.swiper-tab {
    width: 44vw;
    text-align: center;
    line-height: 70rpx;
    background: #fff;
    z-index: 2;
    display: flex;
    justify-content: space-around;
    margin: auto;
}

.swiper-tab-list {
    width: 10vw;
    color: #080808;
}

.chong {
    color: rgb(251,59,68);
}

.shou {
    color: rgb(251,209,50);
}

.bao {
    color: rgb(144,218,100);
}

.zixuan {
    color: #00b1a1;
}

.chong-border {
    border-bottom: 5rpx solid rgb(251,59,68);
}

.shou-border {
    border-bottom: 5rpx solid rgb(251,209,50);
}

.bao-border {
    border-bottom: 5rpx solid rgb(144,218,100);
}

.zixuan-border {
    border-bottom: 5rpx solid #00b1a1;
}

.shaixuan-container {
    position: fixed;
    top: 0;
    left: 100vw;
    width: 100vw;
    height: 100vh;
    z-index: 5;
}

.shaixuan-container .shaixuan-wrap {
    width: 100%;
    height: 100%;
}

.shaixuan-animate {
    animation: shaixuan 0.5s ease forwards;
}

@-webkit-keyframes shaixuan {
    0% {
    }

    100% {
        left: 0;
    }
}

@keyframes shaixuan {
    0% {
    }

    100% {
        left: 0;
    }
}

.shaixuan-animate-out {
    animation: shaixuanOut 0.5s ease forwards;
}

@-webkit-keyframes shaixuanOut {
    0% {
        left: 0;
    }

    100% {
        left: 100vw;
    }
}

@keyframes shaixuanOut {
    0% {
        left: 0;
    }

    100% {
        left: 100vw;
    }
}

.shaixuan-content {
    width: 80.4vw;
    height: 94vh;
    background: #fff;
    position: absolute;
    top: 0;
    right: 0;
    padding: 5vw;
}

.city-wrapper {
    width: 22vw;
    border: 1px solid #f0eff5;
    text-align: center;
    height: 6.67vw;
    line-height: 6.67vw;
    margin: 1vw;
    border-radius: 5px;
}

.cityList {
    flex-wrap: wrap;
    justify-content: flex-start;
    height: 29vh;
    overflow: hidden;
    margin-top: 3vw;
}

.cityList-animate {
    animation: cityListDown 0.3s ease forwards;
}

@-webkit-keyframes cityListDown {
    0% {
    }

    100% {
        height: 64vh;
    }
}

@keyframes cityListDown {
    0% {
    }

    100% {
        height: 64vh;
    }
}

.cityList-animate-out {
    animation: cityListDownOut 0.3s ease forwards;
}

@-webkit-keyframes cityListDownOut {
    0% {
        height: 64vh;
    }

    100% {
        height: 29vh;
    }
}

@keyframes cityListDownOut {
    0% {
        height: 64vh;
    }

    100% {
        height: 29vh;
    }
}

.on {
    color: #fff;
    background: #e9302d;
}

.classtypeList {
    flex-wrap: wrap;
    justify-content: flex-start;
    margin-top: 3vw;
}

.rank-wrapper {
    position: relative;
}

.chongzhi {
    width: 50%;
    height: 100%;
    margin-left: -4vw;
    background-color: #fff;
    border-top-left-radius: 10vw;
    border-bottom-left-radius: 10vw;
}

.queren {
    width: 50%;
    height: 100%;
    color: #fff;
    right: 4vw;
    border-top-right-radius: 10vw;
    border-bottom-right-radius: 10vw;
}

.shaixuan-btn-wrapper {
    height: 10vw;
    position: absolute;
    width: 100%;
    line-height: 10vw;
    text-align: center;
}

.recommendList {
    width: 90.4vw;
    margin: auto;
    background: #fff;
    border-radius: 5px;
}

.beizhu {
    border: 1px solid #f0eff5;
    border-radius: 5px;
    padding: 0 2vw;
}

.createZhiyuan {
    width: 14vw;
    height: 14vw;
    border-radius: 100%;
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: space-around;
    position: fixed;
    bottom: 15vw;
    right: 10vw;
    color: #fff;
}

.shengZhiyuan {
    width: 8vw;
    display: inline-block;
    line-height: 1.5;
}

.point-wrapper {
    position: absolute;
    top: -1vw;
    right: -1vw;
    color: #fff;
    background: #ffb000;
    display: inline-block;
    width: 5.1vw;
    height: 3.24vw;
    text-align: center;
    line-height: 3.24vw;
    border-radius: 10px;
}

.beizhuTankuang {
    position: fixed;
    width: 60vw;
    background: rgba(0,0,0,0);
    color: rgba(255,255,255,0);
    z-index: -1;
    top: 35%;
    left: 50%;
    margin-left: -34vw;
    padding: 4vw;
    border-radius: 5px;
}

.beizhu-animate {
    animation: beizhuShow 0.3s ease forwards;
}

@-webkit-keyframes beizhuShow {
    0% {
        background: rgba(0,0,0,0);
        color: rgba(255,255,255,0);
        z-index: 3;
    }

    100% {
        background: rgba(0,0,0,0.8);
        color: rgba(255,255,255,1);
        z-index: 3;
    }
}

@keyframes beizhuShow {
    0% {
        background: rgba(0,0,0,0);
        color: rgba(255,255,255,0);
        z-index: 3;
    }

    100% {
        background: rgba(0,0,0,0.8);
        color: rgba(255,255,255,1);
        z-index: 3;
    }
}

.beizhu-animate-out {
    animation: beizhuShowOut 0.3s ease forwards;
}

@-webkit-keyframes beizhuShowOut {
    0% {
        background: rgba(0,0,0,0.8);
        color: rgba(255,255,255,1);
        z-index: 3;
    }

    100% {
        background: rgba(0,0,0,0);
        color: rgba(255,255,255,0);
        z-index: -1;
    }
}

@keyframes beizhuShowOut {
    0% {
        background: rgba(0,0,0,0.8);
        color: rgba(255,255,255,1);
        z-index: 3;
    }

    100% {
        background: rgba(0,0,0,0);
        color: rgba(255,255,255,0);
        z-index: -1;
    }
}

.search-wrapper input {
    padding-left: 8vw;
}

.search-wrapper {
    padding: 2.8vw 0;
    position: relative;
}

.checked {
    width: 90.4vw;
    height: 37vw;
    background: rgba(255,255,255,0.8);
    position: fixed;
    z-index: 5;
    right: 4.8vw;
}

.checkedNo {
    width: 0vw;
    height: 0vw;
    overflow: hidden;
}

.checked-animate {
    animation: checked 0.8s ease-in forwards;
}

@-webkit-keyframes checked {
    0% {
        width: 90.4vw;
        height: 37vw;
    }

    100% {
        width: 10vw;
        height: 10vw;
        top: 460px;
        right: 7vw;
        background: rgba(255,255,255,0.7);
    }
}

@keyframes checked {
    0% {
        width: 90.4vw;
        height: 37vw;
    }

    100% {
        width: 10vw;
        height: 10vw;
        top: 460px;
        right: 7vw;
        background: rgba(255,255,255,0.7);
    }
}

.historyScoreLine {
    padding: 0 2vw;
    border: 1px solid #e4e4e4;
    border-radius: 5px;
}