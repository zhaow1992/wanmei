                <view style="width:24vw;text-align:center">{{collegeDetail[0].CollegeModel[0].MinSort==0?'-':collegeDetail[0].CollegeModel[0].MinSort}}</view>
                <view style="width:24vw;text-align:center">{{collegeDetail[0].CollegeModel[0].MinScore==0?'-':collegeDetail[0].CollegeModel[0].MinScore}}</view>
