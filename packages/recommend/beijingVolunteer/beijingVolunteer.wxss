    width: 20vw;
}

.head {
    width: 100%;
    height: 80rpx;
}

.tip {
    position: fixed;
    left: 0;
    right: 0;
    padding: 0 20rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 24rpx;
    color: #999999;
    z-index: 999;
    background: #FFFFFF;
