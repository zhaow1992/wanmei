page {
    background: #F8F5F4;
    overflow: visible;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.info-wrapper {
    padding: 0 2.7vw;
    justify-content: space-between;
    flex-wrap: wrap;
}

.item-wrapper {
    width: 30vw;
    margin-top: 3vw;
    height: 12vw;
    white-space: nowrap;
}

.icon-wrapper {
    width: 6.4vw;
    height: 6.4vw;
    border-radius: 100%;
    justify-content: space-around;
}

.icon-wrapper>image {
    width: 4vw;
    height: 4vw;
}

.text-wrapper {
    margin-left: 2.7vw;
    line-height: 1.5;
    flex-direction: column;
    justify-content: space-between;
    height: 90%;
