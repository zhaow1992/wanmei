.collegesList {
    border-bottom: 1px solid #dedddb;
    padding: 0 0 0 52rpx;
    padding-bottom: 15rpx;
    margin-top: 16rpx;
}

.moreBox {
    right: 0;
    top: 0;
    width: 20px;
    display: flex;
    flex-direction: column;
    height: 146rpx;
    justify-content: center;
}

.more {
    width: 32rpx;
    height: 32rpx;
    justify-content: center;
}

.up_icon,.down_icon {
    width: 28rpx;
    height: 14rpx;
}

.itemCells {
    padding: 20rpx 0;
    padding-right: 32rpx;
    border-top: 1rpx dotted #eee;
}

.itemCells1 {
    align-items: center;
    padding: 15rpx 0;
    padding-right: 32rpx;
    border-top: 1rpx dotted #eee;
}

.tag {
    left: 0;
    top: 32rpx;
    width: 40rpx;
    height: 32rpx;
    text-align: center;
    color: #fff;
    line-height: 32rpx;
}

.tag>image {
    width: 100%;
    height: 100%;
}

.tag>text {
    position: absolute;
    left: 45%;
    top: 0;
    z-index: 99;
    transform: translateX(-50%);
}

.delCollegeMask {
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,0.35);
    width: 100vw;
    height: 100vh;
    z-index: 999;
}

.delCollegeMask .content {
    top: 38%;
    left: 50%;
    transform: translateX(-50%);
    width: 80%;
    padding: 60rpx 44rpx;
    border-radius: 16rpx;
    box-sizing: border-box;
}

.delCollegeMask .tools {
    margin-top: 72rpx;
}

.delCollegeMask .tools>view {
    width: 236rpx;
    height: 72rpx;
    line-height: 72rpx;
    border: 1rpx solid #E9302D;
    border-radius: 18px;
}

.delCollegeMask.show {
    animation: showDel 0.3s ease-in forwards;
}

@-webkit-keyframes showDel {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes showDel {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

.delCollegeMask.hide {
    animation: hideDel 0.3s ease-in forwards;
}

@-webkit-keyframes hideDel {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}

@keyframes hideDel {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}

.del {
