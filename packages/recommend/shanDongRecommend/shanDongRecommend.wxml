<navigationcustomtop navigationText="智能填报"></navigationcustomtop>
<view class="filter">
    <view class="swiper-tab twoFontSize threeFontColor">
        <view bindtap="swichNav" class=" {{currentTab==0?'oneFontSize fourFontColor':''}}" data-current="0" style="{{currentTab==0?'border-bottom:5rpx solid #'+color+';color:#'+color+';':''}}">专业优先</view>
        <view bindtap="swichNav" class=" {{currentTab==1?'oneFontSize fourFontColor':''}}" data-current="1" style="{{currentTab==1?'border-bottom:5rpx solid #'+color+';color:#'+color+';':''}}">院校优先</view>
        <view bindtap="swichNav" class=" {{currentTab==2?'oneFontSize fourFontColor':''}}" data-current="2" style="{{currentTab==2?'border-bottom:5rpx solid #'+color+';color:#'+color+';':''}}">自主填报</view>
        <view bindtap="shaixuan" class="flex fourFontColor twoFontSize" hoverClass="btnHover" style="visibility:{{currentTab==2?'hidden':''}}">
            <view style="{{shaixuan.num>0?'color:#E9302D':''}}">筛选</view>
            <view class="fiveFontSize shaixuan-num" wx:if="{{shaixuan.num>0}}">{{shaixuan.num}}</view>
            <image mode="widthFix" src="/image/siftCollege.png" style="width:24rpx;height:24rpx;margin-left:10rpx;" wx:else></image>
        </view>
    </view>
    <view class="fr ac {{currentTab!=2?'jsb':''}}">
        <view bindtap="changeScore" class="flex fourFontColor twoFontSize scorewrapper" hoverClass="btnHover">
            <view>{{score.totalScore}}分</view>
            <view>{{score.rank?score.rank:'-'}}位</view>
            <view>{{score.chooseLevel}}</view>
            <image mode="widthFix" src="../image/choose_score_icon.png" style="width:22rpx;height:22rpx;"></image>
        </view>
        <view bindtap="changeBatch" class="flex fourFontColor twoFontSize" hoverClass="btnHover">
            <view>{{batch==1?'第一段':'第二段'}}</view>
            <image mode="widthFix" src="../image/choose_score_icon.png" style="width:22rpx;height:22rpx;margin-left:16rpx;"></image>
        </view>
        <view bindtap="addMajor" class="flex fourFontColor twoFontSize" hoverClass="btnHover" style="margin-right:32rpx;" wx:if="{{currentTab!=2}}">
            <image src="/image/search.png" style="width:28rpx;height: 28rpx;margin-right:10rpx;"></image>
            <view>院校/专业</view>
            <view class="pop checkedBackColor whiteFontColor fr ac jc" wx:if="{{keywords.length+uCodes.length>0}}">{{keywords.length+uCodes.length>10?'···':keywords.length+uCodes.length}}</view>
        </view>
    </view>
</view>
<swiper bindchange="swichNav" class="swiper-wrapper" current="{{currentTab}}" style="height:{{HInfo.swiperH}}px;">
    <swiper-item>
        <view class="flex" id="cwb" style="padding:14rpx 32rpx;justify-content:space-between;">
            <view class="flex threeFontSize">
                <view bindtap="chooseCWB" class="CWB {{item.animate}}" data-index="{{index}}" hoverClass="btnHover" style="background:{{item.bgColor}};" wx:for="{{CWBList}}" wx:key="index">{{item.name}}</view>
            </view>
        </view>
        <swiper bindchange="changeCWB" class="swiper-wrapper" current="{{CWBIndex}}" style="height:{{HInfo.swiperH}}px;">
            <swiper-item catchtouchmove="forbidMove" wx:for="{{3}}" wx:for-index="swiperIndex" wx:key="index">
                <scroll-view scrollY bindscrolltolower="scrollCollegeToLower" style="height:{{HInfo.scrollH}}px;">
                    <view class="flex" style="height:100%;justify-content:space-around;" wx:if="{{swiperIndex==0?CCollegeList.showLoad:swiperIndex==1?WCollegeList.showLoad:BCollegeList.showLoad}}">
                        <image mode="widthFix" src="/image/loading_icon1.gif" style="width: 28.06vw;height: 25.14vw;"></image>
                    </view>
                    <block wx:else>
                        <block wx:if="{{swiperIndex==0?CCollegeList.collegeList.length>0:swiperIndex==1?WCollegeList.collegeList.length>0:BCollegeList.collegeList.length>0}}">
                            <view class="recommendList zhejiang-content-color collegeChongName" style="position:relative;margin:0 auto">
                                <view bindtap="collegeUp" class="fr threeFontSize threeFontColor zhejiang-content-color collegeChongName" data-index="{{index}}" data-type="1" data-uCode="{{item.uCode}}" style="position:relative;padding:4vw;background:#fff;border-radius:15rpx;margin-bottom:30rpx" wx:for="{{swiperIndex==0?CCollegeList.collegeList:swiperIndex==1?WCollegeList.collegeList:BCollegeList.collegeList}}" wx:key="index">
                                    <view class="fourFontSize oneFontColor" style="padding-bottom: 70rpx">{{item.probability}}<text class="threeFontSize">%</text>
                                    </view>
                                    <view style="position:absolute;top:2.8vw;right:2.8vw;background:rgba(255,255,255,0.5)"></view>
                                    <view class="fc">
                                        <view style="width:560rpx;margin-left:30rpx;border-bottom:1rpx dashed #E9E9E9;padding-bottom:28rpx;">
                                            <view class="fr ac jsb">
                                                <view bindtap="showMajorDetail" class="oneFontSize bold oneFontColor" data-majorcode="{{item.majorCode}}" style="width:395rpx;">{{item.professionName}}</view>
                                                <view catchtap="tianbaoZY" class="tb {{item.st?'whiteFontColor':''}}" data-collegeCode="{{item.collegeCode}}" data-collegename="{{item.collegeName}}" data-colleges="{{item}}" data-datatype="{{swiperIndex==0?1:swiperIndex==1?2:3}}" data-item="{{item}}" data-majorcode="{{item.majorCode}}" data-uCode="{{item.uCode}}" style="margin-right:30rpx;background:{{item.st?swiperIndex==0?'#FF5053 ':swiperIndex==1?'#F7BA00':'#76CF27':''}}" wx:if="{{item.isFit}}">{{item.st?'已填':'填报'}}</view>
