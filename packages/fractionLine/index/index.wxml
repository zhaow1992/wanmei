                        <view bindtap="showDirectTap" class="flex" style="width:91.47vw;height:10.67vw;border-radius:26.67vw;border: 1px solid #F66666;">
                            <view class="twoFontColor flex textAlign" style="width:14.67vw;font-size:2.67vw;">
