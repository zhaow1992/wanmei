.boxs {
    box-sizing: border-box;
}

.search-box {
    width: 686rpx;
    height: 68rpx;
    border-radius: 36rpx;
    background: #E8E8E8;
    margin-top: 10rpx;
    margin-left: 32rpx;
}

.active {
    min-width: 90rpx;
    padding: 0 18rpx;
    height: 48rpx;
    border-radius: 50rpx;
    line-height: 48rpx;
    text-align: center;
}

.tab-head {
    background: #ececec;
    height: 92rpx;
    width: 750rpx;
    margin-top: 10rpx;
}

.br {
    min-height: 92rpx;
    border-right: 1rpx solid #E2E2E2;
}

.college {
    background: #FAFAFA;
    min-height: 78rpx;
    width: 750rpx;
}

.major {
    background: #ffffff;
    min-height: 78rpx;
    width: 750rpx;
}