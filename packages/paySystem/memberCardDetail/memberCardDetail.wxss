.statusBar {
    display: fixed;
    background: #191919;
    width: 100vw;
    z-index: 2;
    position: fixed;
}

.bar {
    width: 100vw;
    position: fixed;
    left: .0;
    z-index: 2;
    background: #191919;
}

.navigateBack {
    padding: 4vw 10vw 4vw 4vw;
    position: absolute;
}

.navigateBack>image {
    width: 3.2vw;
}

.barText {
    position: absolute;
    width: 64vw;
    text-align: center;
    left: 18vw;
    font-size: 4.6vw;
    color: #fff;
}

.container {
    background: #fff;
    width: 100vw;
    overflow: hidden;
}

.swiperClass {
    height: 43vw;
    width: 100vw;
    position: absolute;
    top: 25vw;
}

.slide-image {
    width: 77.3vw;
    height: 40vw;
    border-radius: 10px;
    position: relative;
    text-align: center;
}

.slide-image>view:first-of-type {
    justify-content: space-between;
    position: absolute;
    top: 3vw;
    left: 5.3vw;
    width: 66vw;
    color: #fff;
}

.slide-image>view:first-of-type>view:last-of-type {
    background: #A6ADC8;
    width: 21.9vw;
    height: 4.53vw;
    text-align: center;
    line-height: 4.53vw;
    border-radius: 4.53vw;
}

.slide-image>view:last-of-type {
    justify-content: space-between;
    position: absolute;
    bottom: 5.3vw;
    left: 5.3vw;
    width: 66vw;
}

.vipBtn {
    font-size: 3.73vw;
    color: #A2A6B5;
    box-shadow: 0 2px 4px 0 rgba(7,27,75,0.12);
    width: 26.7vw;
    height: 8.53vw;
    text-align: center;
    line-height: 8.53vw;
    border-radius: 8.53vw;
    background: linear-gradient(to bottom,#fff,#EEF3FF);
}

.active {
    transform: none;
    transition: all 0.2s ease-in 0s;
}

.quiet {
    transform: scale(0.85);
    transition: all 0.2s ease-in 0s;
}

.commentActive {
    transform: none;
    transition: all 0.2s ease-in 0s;
}

.commentQuiet {
    transform: scale(0.9);
    transition: all 0.2s ease-in 0s;
}

.indicator {
    width: 1.6vw;
    height: 1.6vw;
    border-radius: 100%;
    background: rgba(233,48,45,0.15);
}

.on {
    background: #E9302D;
}

.vipOn {
    background: #fff!important;
}

.header-wrapper {
    height: 73vw;
}

.vip-wrapper>view:first-of-type,.forPeople-wrapper>view:first-of-type {
    text-align: center;
    margin-bottom: 4.8vw;
}

.vip-wrapper>view:last-of-type {
    padding: 0 4.3vw;
    flex-wrap: wrap;
}

.vip-wrapper>view:last-of-type>view {
    flex-direction: column;
    width: 30.4vw;
    margin-bottom: 5.8vw;
}

.vip-wrapper>view:last-of-type>view>view:first-of-type>image:first-of-type {
    width: 13.3vw;
    height: 13.3vw;
}

.vip-wrapper>view:last-of-type>view>view:first-of-type {
    position: relative;
}

.forPeople-wrapper>view:last-of-type {
    line-height: 1;
    background: #fafafa;
    padding: 4vw;
    width: 83.5vw;
    margin: auto;
    box-shadow: 0 0 4px 3px rgba(0,0,0,0.05);
    border-radius: 8px;
}

.forPeople-wrapper>view:last-of-type>view {
    margin-bottom: 3.2vw;
}

.banner-wrapper {
    position: relative;
    margin-top: 4vw;
    width: 100vw;
    height: 56vw;
    line-height: 1.7;
}

.banner-wrapper>image {
    width: 100%;
    height: 100%;
}

.banner-wrapper>view {
    color: #C39569;
    position: absolute;
    left: 8.8vw;
    top: 11.2vw;
}

.spot {
    width: 1.3vw;
    height: 1.3vw;
    border-radius: 100%;
    background: #D7A285;
    margin-right: 1.9vw;
}

.common-wrapper>view:first-of-type {
    text-align: center;
    margin: 6.67vw auto 4.8vw;
}

.common-wrapper>view:last-of-type {
    justify-content: space-around;
    padding: 5vw 0;
}

.problem-wrapper>view:first-of-type {
    text-align: center;
    margin-bottom: 4.8vw;
}

.problem-wrapper>view:last-of-type {
    line-height: 1;
    padding: 4vw;
    width: 83.5vw;
    margin: auto;
    box-shadow: 0 0 4px 3px rgba(0,0,0,0.05);
    border-radius: 8px;
}

.phone {
    text-align: center;
    margin: 6.67vw auto 8.27vw;
}

.pay-wrapper {
    box-shadow: 0 4px 4px 4px rgba(0,0,0,0.05);
    padding: 0 4.3vw;
    position: fixed;
    bottom: 0;
    width: 91.4vw;
}

.money {
    font-size: 6.4vw;
    color: #F54142;
}

.payBtn {
    width: 47.7vw;
    height: 11.73vw;
    text-align: center;
    line-height: 11.73vw;
    color: #fff;
    border-radius: 11.73vw;
    background: linear-gradient(to left,#E9302D,#FF5053);
}