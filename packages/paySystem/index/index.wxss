.titleHeight {
    height: 11.73vw;
}

page {
    background: #f8f5f4;
}

.navigationCustomTop {
}

.navigationCustomTopMain {
    text-align: center;
    border-bottom: 1px solid #f0eff5;
    box-shadow: 0px 2px 3px 0px #f0eff5;
    font-size: 8.44vw;
}

.navigationText {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin: 0 27vw;
}

.backIconButton {
    background: rgba(255,255,255,0.6);
    box-shadow: 0px 0px 3px rgba(255,255,255,1);
    border-radius: 100%;
    display: flex;
    height: 17px;
    margin-right: 10px;
    padding: 4px;
    align-items: center;
    justify-content: space-around;
    width: 17px;
}

.backIconButton>image {
    width: 8px;
    margin-left: -2px;
}

.homeIconButton {
    border-left: 1px solid #666;
    height: 18px;
    display: flex;
    align-items: center;
}

.homeIconButton>image {
    width: 20px;
    padding-left: 15px;
}

.navigationAllIconButton {
    display: flex;
    position: absolute;
    left: 15px;
    bottom: 0;
    align-items: center;
}

.navigationCustomTopAll {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 10;
    background: #fff;
}

.title-font {
    font-size: 4.44vw;
}

@media only screen and (min-width:667px) {
    .title-font {
        font-size: 2.44vw;
    }
}