.moreArrowWH {
    width: 1.6vw;
    height: 3.2vw;
}

.container {
    padding: 0 4.3vw;
}

.wrapAnimate {
    animation: wrapAnimate 0.5s ease-in-out forwards;
}

@-webkit-keyframes wrapAnimate {
    0% {
    }

    100% {
        background: rgba(0,0,0,0.35);
    }
}

@keyframes wrapAnimate {
    0% {
    }

    100% {
        background: rgba(0,0,0,0.35);
    }
}

.wrapAnimateOut {
    animation: wrapAnimateOut 0.4s ease-in-out forwards;
}

@-webkit-keyframes wrapAnimateOut {
    0% {
        background: rgba(0,0,0,0.35);
    }

    100% {
        background: rgba(0,0,0,0);
    }
}

@keyframes wrapAnimateOut {
    0% {
        background: rgba(0,0,0,0.35);
    }

    100% {
        background: rgba(0,0,0,0);
    }
}

.frameAnimate {
    animation: frameAnimate 0.5s ease forwards;
}

@-webkit-keyframes frameAnimate {
    0% {
    }

    100% {
        opacity: 1;
        top: 0vh;
    }
}

@keyframes frameAnimate {
    0% {
    }

    100% {
        opacity: 1;
        top: 0vh;
    }
}

.frameAnimateOut {
    animation: frameAnimateOut 0.4s ease forwards;
}

@-webkit-keyframes frameAnimateOut {
    0% {
        opacity: 1;
        top: 0vh;
    }

    100% {
        opacity: 0;
        top: 100vh;
    }
}

@keyframes frameAnimateOut {
    0% {
        opacity: 1;
        top: 0vh;
    }

    100% {
        opacity: 0;
        top: 100vh;
    }
}

.frame-wrapper {
    position: fixed;
    height: 100vh;
    width: 100vw;
    z-index: 2;
    top: 50vh;
    left: 0;
}

.frame {
    background: #fff;
    position: absolute;
    bottom: 0;
    width: 88.2vw;
    padding: 5.33vw 5.87vw 0;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    z-index: 3;
}

.title-wrapper {
    justify-content: space-between;
    font-size: 4.8vw;
    color: #4a4a4a;
}

.title-wrapper>image {
    width: 3.2vw;
    height: 3.2vw;
    padding: 0 5vw;
    margin-right: -5vw;
}

.flex {
    display: flex;
    align-items: center;
}

.wrap {
    position: fixed;
    z-index: 1;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.content-wrapper {
    line-height: 1;
    padding: 0;
}

.batch-wrapper {
    padding: 4vw 0;
    border-bottom: 1px solid #e9e9e9;
}

.on {
    color: #E9302D;
}